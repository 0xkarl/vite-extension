{
  "name": "Vite",
  "version": "0.1.5",
  "manifest_version": 2,
  "author": "Vite <support@vite.org>",
  "description": "vite",
  "short_name": "vite",
  "icons": {
    "80": "images/vite_80.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "http://localhost:8545/",
    "https://*.infura.io/",
    "activeTab",
    "webRequest",
    "*://*.eth/",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "minimum_chrome_version": "35",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "20": "images/vite_20.png",
      "40": "images/vite_40.png",
      "60": "images/vite_60.png",
      "80": "images/vite_80.png"
    },
    "default_title": "Vite",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "injected-script.js"
  ]
}